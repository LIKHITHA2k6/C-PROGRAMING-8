#include<stdio.h>
#include<conio.h>
void main()
{
int n,i,j,a[10],temp;
clrscr();
printf("enter the no,of elements;/n");
scanf("%d",&n);
printf("Enter the array elements/n");
for(i=0;i<n;i++)
scanf("%d",&a[i]);
printf("The original elemets are/n");
for(i=0;i<n;i++)
printf("5d",a[i]);
for(i=0;i<n-1;i++)
{
for(j=0;j<(n-i)-1;j++)
{
if(a[j]>a{j+1])
{
Temp=a[j];
a[j]=a[j+1];
a[j+1]=temp;
}
}
}
printf("/n The sorted elemets are/n");
for(i=0;i<n;i++)
printf("%d',a[i]);
getch();
}
